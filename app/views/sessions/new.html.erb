<h1 class='center-align'>Login</h1>
<div class="container">

<div class="row">
  <div class="col s1 m1 l2"></div>
  <%= form_with url: sessions_path, local: true, class:"col s10 m10 l8" do |f| %>
    <div class="row">
      <div class="input-field col s12">
        <%= f.label :email %>
        <%= f.email_field :email, class:'validate'%>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s12">
        <%= f.label :password %>
        <%= f.password_field :password, class:"validate" %>
      </div>
    </div>

      <div class="col s6">
        <%= f.submit 'send', class:'btn waves-effect waves-light row' %>
      </div>

  <% end %>
  <div class="col s1 m1 l2"></div>
</div>

</div>
